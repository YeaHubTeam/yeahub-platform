{
	"name": "yeahub-platform",
	"version": "1.1.6",
	"description": "YeaHub is a platform for IT professionals, bringing together learning, career growth, personal development, and a community of like-minded individuals. A React-based platform with integrated Storybook and advanced developer tooling.",
	"main": "src/app/index.tsx",
	"homepage": "https://github.com/YeaHubTeam/yeahub-platform",
	"bugs": {
		"url": "https://github.com/YeaHubTeam/yeahub-platform/issues"
	},
	"scripts": {
		"dev": "webpack serve --mode development --env mode=development",
		"start": "serve -s build",
		"build": "webpack --mode production --env mode=production",
		"docker:dev": "docker-compose --env-file .env.development -f docker-compose.dev.yml up",
		"docker-build:dev": "docker-compose --env-file .env.development -f docker-compose.dev.yml up --build",
		"docker:prod": "docker-compose --env-file .env.production -f docker-compose.yml up",
		"prettier": "npx prettier --write **/*.{ts,tsx,json}",
		"lint:ts": "eslint \"**/*.{ts,tsx}\"",
		"lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
		"lint:css": "npx stylelint \"**/*.css\"",
		"lint:css:fix": "npx stylelint \"**/*.css\" --fix",
		"prepare": "husky",
		"storybook": "storybook dev -p 6006 -c ./config/storybook",
		"storybook:build": "storybook build -c ./config/storybook",
		"test:ui": "npx loki test",
		"test:ui:ok": "npx loki approve",
		"test:ui:json": "node scripts/generate-visual-json-report.js",
		"test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
		"test:unit": "jest --config ./config/jest/jest.config.ts",
		"test:unit:coverage": "jest --coverage  --config ./config/jest/jest.config.ts --collectCoverageFrom='src/**/*.{ts,tsx}' --collectCoverageFrom=!src/**/*.stories.*"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/YeaHubTeam/yeahub-platform"
	},
	"authors": "YeaHubTeam <yeahub@yeahub.ru>",
