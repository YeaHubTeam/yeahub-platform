{
  "name": "yeahub-platform",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.tsx",
  "scripts": {
    "dev": "webpack serve --mode development --env mode=development --open",
    "start": "serve -s build",
    "build": "webpack --mode production --env mode=production",
    "docker:dev": "docker-compose --env-file .env.development -f docker-compose.dev.yml up",
    "docker-build:dev": "docker-compose --env-file .env.development -f docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose --env-file .env.production -f docker-compose.yml up",
    "prettier": "npx prettier --write **/*.{ts,tsx,json}",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:css": "npx stylelint \"**/*.css\"",
    "lint:css:fix": "npx stylelint \"**/*.css\" --fix",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:unit:coverage": "jest --coverage  --config ./config/jest/jest.config.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YeaHubTeam/yeahub-platform"
  },
  "authors": [
    "Ruslan Kuyanets <ruslan.kuyanets@yandex.ru>",
